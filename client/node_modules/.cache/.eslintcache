tems: 'center',\r\n      width: '100%', height: '100vh'\r\n    }}>\r\n        <form style={{ display: 'flex', flexDirection: 'column'}}\r\n          onSubmit={onSubmitHandler}\r\n        >\r\n          <label>Email</label>\r\n          <input type=\"email\" value={Email} onChange={onEmailHandler}></input>\r\n          <label>Password</label>\r\n          <input type=\"password\" value={Password} onChange={onPasswordHandler}></input>\r\n          <label>SecretKey</label>\r\n          <input type=\"password\" value={ApiKey} onChange={onApiKeyHandler}></input>\r\n          <label>AccessKey</label>\r\n          <input type=\"password\" value={AccessKey} onChange={onAccessKeyHandler}></input>\r\n          <br />\r\n          <button>\r\n            Login\r\n          </button>\r\n        </form>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default LoginPage\r\n","C:\\Users\\User\\OneDrive\\Desktop\\vs_code\\Jayco_restart\\coin_trading_project\\client\\src\\components\\views\\PortfolioPage\\PortfolioPage.js",["180"],[],"import React, { useEffect } from 'react';\r\nimport  axios  from 'axios';\r\nimport { useDispatch } from 'react-redux'\r\nimport Footer from '../Footer/Footer';\r\nimport Header from '../Header/Header';\r\nimport '../MainPage/MainPage.css';\r\nimport LandingHeader from '../LandingHeader/LandingHeader';\r\n\r\nfunction PortfolioPage (){ \r\n  useEffect(()=>{\r\n    axios.post('/api/users/coin/balance')\r\n    .then(response => response.data);\r\n  });\r\n\r\n  \r\n    return (\r\n      <div className='layout'>\r\n        <LandingHeader/>\r\n        <Header/>\r\n        이건 포트폴리오 ~~\r\n        <Footer/>\r\n      </div>\r\n    )\r\n}\r\n\r\n\r\nexport default PortfolioPage;","C:\\Users\\User\\OneDrive\\Desktop\\vs_code\\Jayco_restart\\coin_trading_project\\client\\src\\_actions\\user_action.js",["181"],[],"import axios from 'axios';\r\nimport {\r\n    LOGIN_USER,\r\n    REGISTER_USER,\r\n    TRADING_START,\r\n    LOGOUT_USER,\r\n    ApiKey_Set\r\n} from './types';\